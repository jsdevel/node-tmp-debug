    "url": "https://github.com/jsdevel/node-tmp-debug/issues"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "del": "^1.2.0",
    "istanbul": "^0.3.14",
    "jshint": "^2.7.0",
    "mocha": "^2.2.5",
    "should": "^6.0.3"
  },
  "dependencies": {
    "ends-with": "^0.2.0",
    "findup-sync": "^0.2.1",
